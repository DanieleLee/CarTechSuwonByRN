{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@views/*": ["src/views/*"],
      "@ui/*": ["src/ui/*"],
      "@asset/*": ["src/assets/*"],
      "src/*": ["src/*"],
      "#/*": ["./src/*"]
    }
  }
}
